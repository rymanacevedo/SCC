{
  "include": [
    ".react-router/types/**/*",
    "declarations.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "rootDirs": [".", "./.react-router/types"],
  "compilerOptions": {
    "types": ["@react-router/node", "vite/client"],
    "rootDirs": [".", "./.react-router/types"],
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
